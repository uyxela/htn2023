generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "cockroachdb"
    url      = env("DATABASE_URL")
}

model User {
    id           String   @id(map: "primary") @db.Uuid
    email        String   @unique
    createdAt    DateTime @default(now())
    updatedAt    DateTime @updatedAt
    passwordHash String
    name         String
}
